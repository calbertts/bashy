$interpreter = "bash"

print: name
  ~$ --interpreter=$interpreter `echo $name`

pwd:
  ~$ --interpreter=$interpreter "pwd"

list: directory
  print $__flags
  ~$ --interpreter=$interpreter `ls $directory`

read: file
  ~$ --interpreter=$interpreter `cat $file`

filter: pattern
  ~$ --interpreter=$interpreter `echo $__stdin | grep $pattern`
